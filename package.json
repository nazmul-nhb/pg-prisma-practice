    "fix": "nhb-fix",
    "commit": "nhb-commit",
    "count": "nhb-count",
    "module": "nhb-module -t express-prisma-postgres -d src/app/modules",
    "delete": "nhb-delete",
    "secret": "node scripts/generateSecret.mjs"
